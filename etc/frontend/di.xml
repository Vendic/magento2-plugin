        <arguments>
            <argument name="configProviders" xsi:type="array">
                <item name="paynl_payment_config_provider" xsi:type="object">Paynl\Payment\Model\ConfigProvider</item>
            </argument>
        </arguments>
    </type>
    <type name="Magento\Checkout\Block\Onepage">
